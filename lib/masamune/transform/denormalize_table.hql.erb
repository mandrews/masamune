SELECT
  <%- target.select_columns(columns).each do |column, last| -%>
  <%= column %><%= ',' unless last %>
  <%- end -%>
FROM
  <%= target.name %>
ORDER BY
  <%- target.order_by_columns(order_by).each do |column, last| -%>
  <%= column %><%= ',' unless last %>
  <%- end -%>
;
