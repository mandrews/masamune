CREATE TEMPORARY TABLE IF NOT EXISTS <%= target.stage_table.name %> (LIKE <%= target.name %> INCLUDING ALL);

INSERT INTO
  <%= target.stage_table.name %> (<%= target.insert_columns(source).join(', ') %>)
SELECT
  <%- target.insert_values(source).each do |value, last| -%>
  <%= value %><%= ',' unless last %>
  <%- end -%>
FROM
  <%= source.name %>
<%- target.join_conditions(source).each do |(table_name, alias_name), conditions| -%>
LEFT JOIN
  <%= table_name %> AS <%= alias_name %>
ON
  <%= conditions.to_a.join(" AND\n  ") %>
<%- end -%>
;

<%= render 'bulk_upsert.psql.erb', target: target, source: target.stage_table %>
